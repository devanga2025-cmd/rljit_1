{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 10,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico|manifest.json|sw.js|registerSW.js|robots.txt|app-icon.jpeg|pwa-|placeholder.svg|safe-exercises-pregnancy.png).*)",
            "destination": "/index.html"
        }
    ]
}